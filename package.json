{
  "name": "next-js-boilerplate",
  "version": "1.0.0",
  "private": true,
  "keywords": [
    "next.js",
    "tailwindcss",
    "eslint",
    "boilerplate"
  ],
  "description": "Next.js boilerplate with Tailwind CSS, ESLint",
  "engines": {
    "node": ">=18.12.0",
    "vscode": ">=1.80.1"
  },
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "lint:fixAll": "npm run lint -- --fix"
  },
  "dependencies": {
    "next": "14.2.14",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "autoprefixer": "10.4.20",
    "eslint": "8.57.1",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "14.2.14",
    "eslint-plugin-react-hooks": "4.6.2",
    "husky": "9.1.6",
    "postcss": "8.4.49",
    "tailwindcss": "3.4.13"
  },
  "browserslist": [
    "defaults"
  ]
}
