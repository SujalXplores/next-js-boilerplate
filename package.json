{
  "name": "next-js-boilerplate",
  "version": "1.0.0",
  "private": true,
  "keywords": ["next.js", "tailwindcss", "biome", "boilerplate"],
  "description": "Next.js boilerplate with Tailwind CSS, Biome",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
